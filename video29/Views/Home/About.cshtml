<h1>Salam</h1>


<div class="container">
    <div class="row">
        @foreach (var item in ViewBag.Imagesss)
        {
            <div class="col-md-4">
                <div class="card" style="width: 18rem;">
                    <img src="~/@item.Link" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h5 class="card-title">@item.Title</h5>
                    </div>
                </div>
            </div>
        }
    </div>
</div>  